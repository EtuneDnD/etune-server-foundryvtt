{"_id":"AHMUpMbaVkZ5A1KX","img":"systems/pf2e/icons/spells/efficient-apport.webp","name":"Effect: Aid","system":{"badge":null,"description":{"value":"<p>You gain a circumstance bonus to the aided check based on the result of your ally's Aid check.</p>"},"duration":{"expiry":"turn-start","sustained":false,"unit":"rounds","value":1},"level":{"value":1},"rules":[{"choices":[{"label":"-1","value":"-1"},{"label":"+1","value":"1"},{"label":"+2","value":"2"},{"label":"+3","value":"3"},{"label":"+4","value":"4"}],"flag":"aidBonus","key":"ChoiceSet","prompt":"Choose the bonus from Aid"},{"key":"FlatModifier","selector":"all","type":"circumstance","value":"{item|flags.pf2e.rulesSelections.aidBonus}"}],"source":{"value":"Pathfinder Core Rulebook"},"start":{"initiative":null,"value":0},"target":null,"tokenIcon":{"show":true},"traits":{"custom":"","rarity":"common","value":[]},"slug":"effect-aid","schema":{"version":0.8,"lastMigration":null}},"type":"effect","flags":{"core":{"sourceId":"Compendium.pf2e.other-effects.AHMUpMbaVkZ5A1KX"}}}
